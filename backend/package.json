{
  "name": "e-learning-backend",
  "version": "1.0.0",
  "description": "E-learning platform backend API",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node -e \"require('./src/utils/migrationRunner').runMigrations().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "migrate:rollback": "node -e \"require('./src/utils/migrationRunner').rollbackMigrations().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "db:sync": "node -e \"require('./src/config/database').syncDatabase({ force: false }).then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "db:reset": "node -e \"require('./src/config/database').syncDatabase({ force: true }).then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "seed": "node src/utils/seed.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "e-learning",
    "education",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "socket.io": "^4.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  }
}
